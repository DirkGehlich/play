{
  "name": "battlesnakeio-play",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "lint": "yarn lint:eslint",
    "lint:eslint": "./node_modules/.bin/eslint '**/*.js'",
    "lint:prettier": "./node_modules/.bin/prettier --config package.json '**/*.js' --list-different",
    "prettier": "./node_modules/.bin/prettier --config package.json '**/*.js' --write",
    "test": "./node_modules/.bin/cypress run",
    "test:gui": "./node_modules/.bin/cypress open"
  },
  "dependencies": {
    "cypress": "3.1.3",
    "eslint": "5.9.0",
    "eslint-plugin-cypress": "2.1.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-mocha": "5.2.0",
    "mocha": "5.2.0",
    "prettier": "1.15.3",
    "webpack": "4.27.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100
  }
}
